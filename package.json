{
  "name": "intl-pluralrules",
  "version": "1.0.3",
  "description": "Intl.PluralRules polyfill",
  "keywords": [
    "unicode",
    "cldr",
    "i18n",
    "internationalization",
    "pluralization"
  ],
  "author": "Eemeli Aro <eemeli@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/eemeli/intl-pluralrules#readme",
  "repository": "eemeli/intl-pluralrules",
  "files": [
    "factory.js",
    "plural-rules.js",
    "plural-rules.d.ts",
    "polyfill.js"
  ],
  "main": "polyfill.js",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ]
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "mjs"
    ],
    "testMatch": [
      "**/*.test.mjs"
    ],
    "transform": {
      "\\.mjs$": "babel-jest"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "make-plural": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0"
  },
  "scripts": {
    "build": "babel -d . src/ --ignore '**/*.test.mjs'",
    "prepublishOnly": "npm test && npm run build",
    "test": "jest"
  }
}
