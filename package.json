{
  "name": "IntlPluralRules",
  "version": "0.1.0",
  "description": "Intl.PluralRules polyfill",
  "keywords": [
    "unicode",
    "cldr",
    "i18n",
    "internationalization",
    "pluralization"
  ],
  "author": "Eemeli Aro <eemeli@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/eemeli/IntlPluralRules",
  "repository": {
    "type": "git",
    "url": "https://github.com/eemeli/IntlPluralRules.git"
  },
  "bugs": {
    "url": "https://github.com/eemeli/make-plural.js/issues"
  },
  "files": [
    "polyfill.*"
  ],
  "main": "polyfill",
  "files": [
    "lib/messageformat.js",
    "lib/messageformat-parser.js",
    "bin/messageformat.js",
    "example",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "babel": "*",
    "babel-eslint": "*",
    "babelify": "*",
    "browserify": "*",
    "eslint": "*",
    "expect.js": "*",
    "http-server": "*",
    "make-plural": "^3.0.1",
    "mocha": "*",
    "uglify-js": "*"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "curly": [
        2,
        "multi-line"
      ],
      "eqeqeq": [
        1,
        "smart"
      ],
      "quotes": [
        1,
        "single",
        "avoid-escape"
      ]
    }
  },
  "scripts": {
    "test": "make test"
  }
}
